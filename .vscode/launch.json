    "version": "0.2.0",
    "configurations": [
        {
            "type": "python",
            "name": "FastAPI",
            "request": "launch",
            "module": "uvicorn",
            "args": ["src.fastp.main:app", "--reload"],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "com.hry.firstjava.App",
            "projectName": "java-project"
        }
    ]
}